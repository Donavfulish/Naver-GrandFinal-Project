    "express": "^5.1.0",
    "joi": "^18.0.1",
    "node-cache": "^5.1.2",
    "prisma": "^6.19.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
